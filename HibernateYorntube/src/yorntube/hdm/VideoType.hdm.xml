<hibernate-mapping>
    <class name='yorntube.persistence.VideoType' table='videotypes'>
        <id name='type_id' type='long' column='VT_ID'>
            <generator class='increment'/>
        </id>
        <property name='type' column='TYPE'/>
    </class>
</hibernate-mapping>